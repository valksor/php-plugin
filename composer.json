{
    "name": "valksor/php-plugin",
    "description": "WIP",
    "license": "BSD-3-Clause",
    "type": "composer-plugin",
    "homepage": "https://github.com/valksor/php-plugin",
    "require": {
        "php": ">=8.4",
        "composer-plugin-api": "^2",
        "composer/composer": "^2",
        "symfony/console": "*",
        "symfony/flex": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "conflict": {
        "doctrine/dbal": "<4.0",
        "friendsofphp/php-cs-fixer": "<3.81.0",
        "symfony/framework-bundle": "<7.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "ValksorPlugin\\": "src/ValksorPlugin/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "class": "ValksorPlugin\\ValksorFlex"
    }
}
